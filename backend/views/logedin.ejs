        <form action="http://localhost:5002/" method="POST" enctype="multipart/form-data"
          class="flex max-w-[480px] flex-wrap gap-4 px-4 py-3">
          <div class="flex flex-wrap items-end w-full gap-4">
            <label class="flex flex-col min-w-40 flex-1">
              <p class="text-[#111518] text-base font-medium leading-normal pb-2">Upload your video file</p>
              <input type="file" name="videoFile" accept="video/*"
                class="form-input flex w-full min-w-0 flex-1 rounded-xl text-[#111518] focus:outline-0 focus:ring-0 border border-[#dbe1e6] bg-white focus:border-[#dbe1e6] placeholder:text-[#60778a] p-[15px] text-base font-normal leading-normal"
                required />
            </label>
          </div>
          <div class="flex w-full px-4 py-3">
            <button type="submit"
              class="flex min-w-[84px] max-w-[480px] cursor-pointer items-center justify-center overflow-hidden rounded-xl h-10 px-4 flex-1 bg-[#2094f3] text-white text-sm font-bold leading-normal tracking-[0.015em]">
              <span class="truncate">Upload</span>
            </button>
          </div>
        </form>


        <h3 class="text-[#111518] text-lg font-bold leading-tight tracking-[-0.015em] px-4 pb-2 pt-4">Transcript</h3>
        <p class="text-[#111518] text-base font-normal leading-normal pb-3 pt-1 px-4">
          <%=tran%>
        </p>
        <h3 class="text-[#111518] text-lg font-bold leading-tight tracking-[-0.015em] px-4 pb-2 pt-4">Ask a question or
          search for a keyword</h3>
          <div class="flex max-w-[600px] flex-wrap gap-4 px-4 py-3">
            <span><%=ans%></span>
            <form class="flex flex-col gap-4 w-full" method="post" action="/question">

              <label class="flex flex-col">
                <p class="text-[#111518] text-base font-medium leading-normal pb-2">Search transcript</p>
                <input 
                  placeholder="What would you like to find in this transcript?"
                  class="form-input flex w-[600px] resize-none overflow-hidden rounded-xl text-[#111518] focus:outline-0 focus:ring-0 border-none bg-[#f0f2f5] focus:border-none h-14 placeholder:text-[#60778a] p-4 text-base font-normal leading-normal"
                  name="searchQuery"
                  value=""
                />
              </label>
              <input type="hidden" name="transcript" value="<%=tran%>" />
              <div class="flex w-[15px] px-2 py-3">
                <button type="submit"
                  class="flex min-w-[84px] max-w-[480px] cursor-pointer items-center justify-center overflow-hidden rounded-xl h-10 px-4 flex-1 bg-[#2094f3] text-white text-sm font-bold leading-normal tracking-[0.015em]">
                  <span class="truncate">Search</span>
                </button>
              </div>
            </form>
          </div>
          

